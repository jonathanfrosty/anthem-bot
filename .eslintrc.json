{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": 0,
    "max-len": ["error", { "code": 150 }],
    "no-param-reassign": 0,
    "no-use-before-define": 0,
    "no-console": 0,
    "no-plusplus": 0,
    "no-new": 0,
    "import/extensions": 0,
    "object-curly-newline": ["error", { "ImportDeclaration": { "multiline": true } }],
    "import/no-dynamic-require": 0
  }
}